    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.jlobatonm.socialclub_app.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_reservas"
        android:name="com.jlobatonm.socialclub_app.ui.reservas.ReservasFragment"
        android:label="Reservas"
        tools:layout="@layout/fragment_reservas" />

    <fragment
        android:id="@+id/navigation_mis_reservas"
        android:name="com.jlobatonm.socialclub_app.ui.mis_reservas.MisReservasFragment"
